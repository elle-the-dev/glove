{
    "name": "ellehamilton/glove",
    "description": "Exception handling for Laravel that catches like a glove.",
    "keywords": ["laravel", "exceptions", "exception", "handling"],
    "license": "MIT",
    "require": {
        "php": "7.*|8.*",
        "illuminate/contracts": "8.*",
        "illuminate/support": "8.*",
        "symfony/console": "5.*",
        "symfony/debug": "^3.1|4.*",
        "symfony/http-foundation": "5.*",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "9.*",
        "orchestra/testbench": "6.*",
        "filp/whoops": "2.*"
    },
    "autoload": {
        "psr-4": {
            "ElleHamilton\\Glove\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElleHamilton\\Tests\\Glove\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElleHamilton\\Glove\\Providers\\GloveServiceProvider"
            ]
        }
    }
}
